#video
	padding 180px 0 80px
.video-text
	font-size: 35px
	font-weight: bold
	margin-top: 100px
.video-wrapper-video
	&:after
		height 100px
	iframe
		width: 75%;
		height: 78%;
		top: 19px;
		left 2px
		@media screen and (max-width 385px)
			top 15px
	&-laptop
		position absolute
		top 0
		left 0
		right 0
		width 100%